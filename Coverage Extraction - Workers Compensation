      "bodily_injury_by_disease_policy_limit": "",
      "bodily_injury_by_disease_each_employee": "",
      "employers_liability_limit": "",
      "stop_gap": "",
      "experience_modifier": ""
    },
    "exposures": {
      "class_codes": [
        {
          "class_code": "",
          "description": "",
          "state": "",
          "annual_payroll": "",
          "rate": ""
        }
      ],
      "ownership_schedule": [
        {
          "name": "",
          "ownership_percent": "",
